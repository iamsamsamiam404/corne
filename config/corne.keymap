                default_layer2 {

                        bindings = <
   &none &kp Q &kp D &kp R &kp W &kp B        &kp J &kp F  &kp U     &kp P   &kp SEMI &none
   &none &kp A &kp S &kp H &kp T &kp G        &kp Y &kp N  &kp E     &kp O   &kp I &none
   &none &kp Z &kp X &kp M &kp C &kp V        &kp K &kp L  &kp COMMA &kp DOT &kp FSLH &none
                &kp LSHIFT &mo 1 &mo 2 &kp SPACE &mo 3 &mo 4
                        >;
                };
                
                SUPER {

                        bindings = <
   &none &none &none &none &none &none        &none &none &none &none &none &none  
   &none &none &none &none &kp A &none        &none &kp A &none &none &none &none
   &none &none &none &none &none &none        &none &none &none &none &none &none 
                     &none &mo 1 &none        &none &none &none
                        >;
                };
                CODE {

                        bindings = <
   &none &none &none &kp ENTER &none &none        &none &none &none &none &none &none
   &none &none &none &none &kp B &none            &none &kp B &none &none &none &none
